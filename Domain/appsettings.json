{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "AppSettings": {
    "ApiKeyOptions": {
      "Key": "api-key",
      "Value": "my-secret-api-key"
    },
    "DbOptions": {
      "ConnectionString": "Data Source=example.db"
    }
  }
}
